package testing;

public class JunitTesting {
	public int square(int x) {
		return x*x;
	}
	
	public int doubl(int x, int y) {
		return x+x;
	}
	
	public int substract(int x, int y) {
		return x-y;
	}
	
	public int devide(int x, int y) {
		return x/y;
	}
	
	public int modulus(int x, int y) {
		return x%y;
	}
}
